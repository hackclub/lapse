{
    "display_information": {
        "name": "Lapse (Mock Environment)"
    },
    "oauth_config": {
        "redirect_urls": [
            "https://lapse.hackclub.com/api/authSlack",
            "http://localhost:3000/api/authSlack"
        ],
        "scopes": {
            "user": [
                "identity.avatar",
                "identity.basic",
                "identity.email"
            ]
        }
    },
    "settings": {
        "org_deploy_enabled": false,
        "socket_mode_enabled": false,
        "token_rotation_enabled": false
    }
}